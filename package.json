{
  "name": "cv2latex",
  "version": "1.0.0",
  "description": "Convert your CV/resume from plain text or Markdown to professionally formatted LaTeX code",
  "type": "module",
  "author": "Tirth Kanani",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tirth8205/CV2Latex.git"
  },
  "homepage": "https://github.com/tirth8205/CV2Latex#readme",
  "bugs": {
    "url": "https://github.com/tirth8205/CV2Latex/issues"
  },
  "keywords": [
    "cv",
    "resume",
    "latex",
    "converter",
    "markdown",
    "pdf",
    "job-application",
    "professional"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "latex.js": "^0.12.6",
    "prismjs": "^1.29.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/prismjs": "^1.26.3",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "@vitest/coverage-v8": "^4.0.17",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.14",
    "typescript": "~5.6.2",
    "vite": "^5.4.10",
    "vitest": "^4.0.17"
  }
}
